{
    "swagger": "2.0",
    "info": {
        "description": "PomfCrypt is a service which offers simple encrypted file uploading",
        "title": "PomfCrypt Backend",
        "termsOfService": "https://github.com/pomfcrypt/pomfcrypt",
        "contact": {
            "name": "Daniel Malik",
            "url": "https://github.com/pomfcrypt/pomfcrypt",
            "email": "mail@fronbasal.de"
        },
        "license": {
            "name": "MIT License"
        },
        "version": "0.1"
    },
    "host": "localhost:3000",
    "basePath": "/api/v1",
    "paths": {
        "/data": {
            "put": {
                "description": "Upload a new file",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Upload route",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/model.FileResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/routes.APIErrorMessage"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/routes.APIErrorMessage"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "model.FileResponse": {
            "type": "object",
            "properties": {
                "hash": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "uploaded_at": {
                    "type": "integer"
                }
            }
        },
        "routes.APIErrorMessage": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "error"
                },
                "message": {
                    "type": "string"
                }
            }
        }
    }
}